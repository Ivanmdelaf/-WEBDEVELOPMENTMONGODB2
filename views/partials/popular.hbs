<div class="card card-default mb-2">
    <div class="card-header">
        <h3 class="card-title">Most Popular</h3>
    </div>
    <div class="card-body ">
        {{#each sidebar.popular}}
            <div class="col-md-4 text-center" style="paddingbottom:.5em;">
                <a href="/images/{{uniqueId}}">
                    <img src="/public/upload/{{filename}}" style="width: 75px; height: 75px;" class="img-thumbnail">
                </a>
            </div>
        {{/each}}
    </div>
</div>